    "sails": "test \"$NODE_ENV\" != production && sails lift",
    "start": "NODE_ENV=production node app.js",
    "test": "npm run lint && npm run htmlhint && npm run mocha && npm run coverage",
    "lint": "eslint ./api --fix --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  ./api/*.js meets .eslintrc standards'",
    "mocha": "nyc ./node_modules/.bin/_mocha --recursive ./test/lifecycle.js ./test",
    "coverage": "nyc report",
    "htmlhint": "htmlhint views",
    "docs": "rm -r docs 2> /dev/null; jsdoc --recurse -c ./jsdoc.conf -d docs api"
  },
